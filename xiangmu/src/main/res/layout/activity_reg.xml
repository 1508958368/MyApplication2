<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:layout_height="match_parent" android:layout_width="match_parent" xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView android:layout_height="wrap_content" android:layout_width="match_parent" android:text="注册页面" android:textSize="25dp" android:gravity="center"/>
    <EditText android:id="@+id/num" android:layout_height="50dp" android:layout_width="match_parent" android:hint="请输入手机号" android:background="@drawable/et" android:layout_marginRight="40dp" android:layout_marginLeft="40dp" android:layout_marginTop="50dp"/>
    <EditText android:id="@+id/pass" android:layout_height="50dp" android:layout_width="match_parent" android:hint="请输入密码" android:background="@drawable/et" android:layout_marginRight="40dp" android:layout_marginLeft="40dp" android:layout_marginTop="50dp"/>
    <EditText android:id="@+id/pass2" android:layout_height="50dp" android:layout_width="match_parent" android:hint="请确认输入密码" android:background="@drawable/et" android:layout_marginRight="40dp" android:layout_marginLeft="40dp" android:layout_marginTop="50dp"/>
    <EditText android:id="@+id/email" android:layout_height="50dp" android:layout_width="match_parent" android:hint="请输入邮箱" android:background="@drawable/et" android:layout_marginRight="40dp" android:layout_marginLeft="40dp" android:layout_marginTop="50dp"/>
    <Button android:id="@+id/zhuce" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="注册" android:layout_marginLeft="20dp" android:layout_marginTop="50dp"/>
</LinearLayout>
